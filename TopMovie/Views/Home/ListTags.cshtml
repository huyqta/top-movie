@model ListTagModel;
<cache vary-by="@Model">
    <div class="row">
        <div class="col-xs-12">
            <h2>@Model.PageTitle</h2>
        </div>
        <hr />
    </div>
    <div>
        <ul class="list-tags">
            @foreach (var tag in Model.ListTags)
            {

                <li><a href="@Url.Content("actress/" + tag.name)" target="_self">@tag.name</a><span>(@tag.quantity)</span></li>
            }
        </ul>
    </div>
</cache>